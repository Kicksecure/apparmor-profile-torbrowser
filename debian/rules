%:
	dh $@

override_dh_install:
	dh_apparmor --profile-name='home.tor-browser.firefox'
	dh_apparmor --profile-name='home.tor-browser.start-tor-browser'

override_dh_installchangelogs:
	dh_installchangelogs changelog.upstream upstream
