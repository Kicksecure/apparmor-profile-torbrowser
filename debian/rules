#export DH_VERBOSE=1

%:
	dh $@

override_dh_install:
	dh_apparmor --profile-name='home.*.tor-browser_*.Browser.firefox'
	dh_apparmor --profile-name='home.*.tor-browser_*.Browser.start-tor-browser'

override_dh_installchangelogs:
	dh_installchangelogs changelog.upstream upstream
